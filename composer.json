{
    "name": "oxygen/mod-security",
    "description": "Security Utilities",
    "license": "MIT",
    "keywords": [
        "cms",
        "framework",
        "OxygenExtension"
    ],
    "authors": [
        {
            "name": "Chris Chamberlain",
            "email": "c.chamberlain@iinet.net.au"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "oxygen/core": "~0.4.0",
        "oxygen/data": "~0.6.0",
        "guzzlehttp/guzzle": "~5.0",
        "illuminate/support": "~5.1.0"
    },
    "autoload": {
        "psr-4": {
            "OxygenModule\\Security\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "title": "Oxygen Module - Security",
        "readme": "oxygen.md",
        "changes": "changes.md",
        "icon": "icon.png",
        "providers": [
            {
                "name": "Security",
                "class": "OxygenModule\\Security\\SecurityServiceProvider",
                "description": "Security Utilities"
            }
        ]
    }
}
